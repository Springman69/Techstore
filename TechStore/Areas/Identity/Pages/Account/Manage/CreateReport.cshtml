@page
@model TechStore.Areas.Identity.Pages.Account.Manage.CreateReportModel
@{
    <form method="post">
        <div class="form-group">
            <label for="title">Tytuł zgłoszenia:</label>
            <input type="text" class="form-control" id="title" asp-for="@Model.report.Title">
        </div>
        <div class="form-group">
            <label for="description">Opis zgłoszenia:</label>
            <textarea class="form-control" id="description" rows="5" asp-for="@Model.report.Description"></textarea>
        </div>
       
        <button type="submit" class="btn btn-primary">Zapisz</button>
    </form>

}
